<div class="s_article_container non_hover row">
  <div class="row"><div class='article-actions'>
    <ul class="list-inline pull-right">
      <% if user_signed_in? %>
        <li><%= link_to 'Delete', article_path(@article), 
          :confirm => "Are you sure you want to delete this article?", 
          :method => :delete, :action => "destroy", :class=>'muted' %>
        </li>
        <li><%= link_to 'Edit', edit_article_path(@article), :class=>'muted' %></li>
      <% end %>
      <li><%= link_to 'Back', root_path, :class=>'muted' %></li>
    </ul>
  </div></div>
  <div class=row">
    <%= render 'article' %>
  </div>
</div>
